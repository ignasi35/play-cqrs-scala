
GET        /balance/:accountNum        controllers.AccountController.balance(accountNum: String)
POST       /deposit/:accountNum        controllers.AccountController.deposit(accountNum: String)
POST       /withdraw/:accountNum       controllers.AccountController.withdraw(accountNum: String)

GET        /assets/*file                 controllers.Assets.at(path="/public", file)
GET        /favicon.ico                  controllers.Assets.at(path="/images", file ="favicon.png")
